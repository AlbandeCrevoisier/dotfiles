sticky yes

unbind-key all
unbind-mouse all

bind-key 4-r restart
bind-key 4-q quit

command term "st tmux"

bind-key 4-a menu-exec
bind-key 4-e "firefox"
bind-key 4-u terminal

bind-key C4-o "ksc"
bind-key M4-o "nsc"
bind-key CM4-o "nsc-iiyama"

bind-key 4-w window-close
bind-key 4-f window-fullscreen
bind-key 4-m window-maximize

bind-key 4-Tab window-cycle

bind-key 4-v window-vtile
bind-key 4-z window-htile

bind-mouse 4-1 window-resize
bind-mouse 4-3 window-move
bind-mouse 4-2 menu-window

bind-key 4-h group-only-1
bind-key 4-t group-only-2
bind-key 4-n group-only-3
bind-key 4-s group-only-4

bind-key 4S-h window-movetogroup-1
bind-key 4S-t window-movetogroup-2
bind-key 4S-n window-movetogroup-3
bind-key 4S-s window-movetogroup-4

bind-key 4SC-h group-toggle-1
bind-key 4SC-t group-toggle-2
bind-key 4SC-n group-toggle-3
bind-key 4SC-s group-toggle-4
